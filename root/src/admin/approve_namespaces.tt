[% META title = 'Approve Namespaces' %]

<form method="post" action="[% c.uri_for('/admin/approve_namespaces') %]">
<input type='radio' name='approve_item' value='1' /> Channel Namespaces ([% pending_channel %] pending)<br />
<input type='radio' name='approve_item' value='2' /> Cloak Namespaces ([% pending_cloak %] pending)<br />
<div class="form-actions"><input type="submit" value="Submit" class="btn btn-primary" /></div>
</form>
